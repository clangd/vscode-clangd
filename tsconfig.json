{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2022",
        "outDir": "out",
        "lib": [
            "ES2022",
        ],
        "sourceMap": true,
        "rootDir": ".",
        "strict": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true
    },
    "exclude": [
        "node_modules",
        ".vscode-test"
    ]
}
